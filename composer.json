{
  "name": "slatch/telegram-bot-client",
  "type": "library",
  "description": "PHP client for telegram bots api",
  "keywords": ["php", "telegram", "client", "bot", "api"],
  "homepage": "https://leonov.xyz",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrii Leonov",
      "email": "andrew@leonov.xyz"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-curl": "*",
    "ext-json": "*",
    "psr/http-message": "^1.0",
    "guzzlehttp/psr7": "^2.0",
    "psr/http-client": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Slatch\\TelegramBotClient\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "require-dev": {
    "codeception/codeception": "^4.1",
    "codeception/module-asserts": "^1.3",
    "codeception/module-phpbrowser": "^1.0"
  }
}
